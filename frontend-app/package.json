{
  "name": "frontend-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write --plugin-search-dir=. ./src/",
    "watch:tailwind": "TAILWIND_MODE=watch NODE_ENV=development postcss src/global.css -o public/styles/global-compiled.css -w",
    "build:tailwind": "TAILWIND_MODE=build NODE_ENV=production postcss src/global.css -o public/styles/global-compiled.css"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.7",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.37.0",
    "vite": "^2.2.3"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "postcss-cli": "^8.3.1",
    "socket.io-client": "^4.0.1",
    "tailwindcss": "^2.1.2"
  }
}
